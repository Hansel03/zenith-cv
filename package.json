{
  "name": "zenith",
  "version": "0.0.1",
  "private": true,
  "description": "Template for creating a comprehensive virtual resume for developers.",
  "homepage": "https://github.com/KonradSzwarc/zenith#readme",
  "bugs": "https://github.com/KonradSzwarc/zenith/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/KonradSzwarc/zenith.git"
  },
  "license": "MIT",
  "author": "Konrad Szwarc <konrad.szwarc.dev@gmail.com> (https://github.com/KonradSzwarc)",
  "type": "module",
  "engines": {
    "node": "22.x",
    "npm": "10.x",
    "pnpm": "please-use-npm",
    "yarn": "please-use-npm"
  },
  "scripts": {
    "postinstall": "npm run generate:favicons",
    "dev": "astro dev",
    "build": "tsx cli build",
    "preview": "astro preview",
    "check": "concurrently \"eslint .\" \"astro check\" \"prettier --check .\" -n eslint,astro,prettier -c magenta,red,yellow",
    "check:format": "prettier --check .",
    "format": "prettier --write .",
    "generate": "tsx cli generate",
    "generate:pdf": "tsx cli pdf",
    "generate:pdf:watch": "nodemon --exec \"tsx cli pdf\"",
    "generate:og": "tsx cli og",
    "generate:og:watch": "nodemon --exec \"tsx cli og\"",
    "generate:favicons": "tsx cli favicons",
    "update:colors": "tsx cli colors",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/mdx": "4.3.13",
    "@astrojs/sitemap": "3.7.0",
    "@floating-ui/dom": "1.7.5",
    "@fontsource-variable/inter": "5.2.8",
    "@playform/compress": "0.2.1",
    "astro": "5.17.1",
    "astro-icon": "1.1.5",
    "astro-meta-tags": "0.4.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "i18next": "25.8.4",
    "iconify-icon": "3.0.2",
    "marked": "17.0.1",
    "mime": "4.1.0",
    "nanoid": "5.1.6",
    "polished": "4.3.1",
    "tailwind-merge": "3.4.0",
    "throttle-debounce": "5.0.2",
    "type-fest": "5.4.3"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "1.10.6",
    "@eslint/js": "9.39.2",
    "@percy/cli": "1.31.8",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "22.19.9",
    "@types/throttle-debounce": "5.0.2",
    "chalk": "5.6.2",
    "commander": "14.0.3",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "dotenv": "17.2.4",
    "eslint": "9.39.2",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tailwindcss": "4.0.0-beta.0",
    "execa": "9.6.1",
    "favicons": "7.2.0",
    "husky": "9.1.7",
    "nodemon": "3.1.11",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1",
    "puppeteer": "24.37.2",
    "start-server-and-test": "2.1.3",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0"
  }
}
